'99pay') {\r\n      porcentagemCDIProduto = 100;\r\n      taxaMensal = (valorCDI * porcentagemCDIProduto) / 100 / 12;\r\n    } else if (params.produto === 'iti') {\r\n      porcentagemCDIProduto = 100;\r\n      taxaMensal = (valorCDI * porcentagemCDIProduto) / 100 / 12;\r\n    } else if (params.produto === 'will') {\r\n      porcentagemCDIProduto = 100;\r\n      taxaMensal = (valorCDI * porcentagemCDIProduto) / 100 / 12;\r\n    } else if (params.produto === 'neon') {\r\n      porcentagemCDIProduto = 101;\r\n      taxaMensal = (valorCDI * porcentagemCDIProduto) / 100 / 12;\r\n    } else if (params.produto === 'tesouro') {\r\n      taxaMensal = ValorTesouro2027 / 12;\r\n    } else if (params.produto === 'fgts') {\r\n      taxaMensal = 3 / 12;\r\n    } else {\r\n      taxaMensal = (valorSelic * 70) / 100 / 12;\r\n    }\r\n    taxaMensal = taxaMensal / 100;\r\n\r\n    while (tempoFinal <= tempoMeses) {\r\n      montante = parseFloat(capitalInicial) * (1 + parseFloat(taxaMensal));\r\n      capitalInicial = parseFloat(montante) + parseFloat(valorMensal);\r\n      tempoFinal += 1;\r\n      // console.log(montante);\r\n    }\r\n    montante = parseFloat(montante) + parseFloat(valorMensal);\r\n    console.log(capitalInicial);\r\n    console.log(montante);\r\n\r\n    let produtoBruto = montante;\r\n    let produtoIr = calcula_ir(valorTotalInvestido, produtoBruto, tempoMeses);\r\n    let produtoLiquido = produtoBruto - produtoIr;\r\n\r\n    setInvestimento(valorTotalInvestido);\r\n    setTempo(tempoMeses);\r\n    setRetornoBruto(produtoBruto);\r\n    setRetornoLiquido(produtoLiquido);\r\n    setImposto(produtoIr);\r\n  }\r\n\r\n  function calcula_ir(valorInvestido, valorBruto, qtdeMeses) {\r\n    let rendimento = valorBruto - valorInvestido;\r\n    let ir = 0;\r\n\r\n    if (qtdeMeses < 6) {\r\n      ir = (rendimento * 22.5) / 100;\r\n    } else if (qtdeMeses > 6 && qtdeMeses <= 12) {\r\n      ir = (rendimento * 20) / 100;\r\n    } else if (qtdeMeses > 12 && qtdeMeses < 24) {\r\n      ir = (rendimento * 17.5) / 100;\r\n    } else if (qtdeMeses >= 24) {\r\n      ir = (rendimento * 15) / 100;\r\n    }\r\n\r\n    if (params.produto === 'fgts') {\r\n      ir = 0;\r\n    }\r\n    return ir;\r\n  }\r\n\r\n  async function handleSubmit(event) {\r\n    event.preventDefault();\r\n\r\n    calculaJurosCompostos(inputValor, inputPeriodo, inputValorMensal);\r\n  }\r\n\r\n  function getTitle() {\r\n    let titulo = '';\r\n\r\n    if (params.valor > 0) {\r\n      titulo = 'Quanto Rende ' + params.valor + ' Reais ';\r\n    } else {\r\n      titulo = 'Quanto Rende Seu Dinheiro ';\r\n    }\r\n\r\n    if (params.produto === 'poupanca') {\r\n      titulo = titulo + 'na Poupana';\r\n    } else if (params.produto === 'mercadopago') {\r\n      titulo = titulo + 'no Mercado Pago';\r\n    } else if (params.produto === 'nubank') {\r\n      titulo = titulo + 'no NuBank';\r\n    } else if (params.produto === 'picpay') {\r\n      titulo = titulo + 'no PicPay';\r\n    } else if (params.produto === 'pagbank') {\r\n      titulo = titulo + 'no PagBank';\r\n    } else if (params.produto === 'next') {\r\n      titulo = titulo + 'no Banco Next';\r\n    } else if (params.produto === 'inter') {\r\n      titulo = titulo + 'no Banco Inter';\r\n    } else if (params.produto === '99pay') {\r\n      titulo = titulo + 'na 99Pay';\r\n    } else if (params.produto === 'neon') {\r\n      titulo = titulo + 'no Banco Neon';\r\n    } else if (params.produto === 'iti') {\r\n      titulo = titulo + 'no Iti';\r\n    } else if (params.produto === 'tesouro') {\r\n      titulo = titulo + 'no Tesouro Selic';\r\n    } else if (params.produto === 'fgts') {\r\n      titulo = titulo + 'no FGTS';\r\n    } else if (params.produto === 'will') {\r\n      titulo = titulo + 'no Will Bank';\r\n    } else {\r\n      titulo = titulo + 'na Poupana';\r\n    }\r\n    return titulo;\r\n  }\r\n\r\n  function retornaURLBlog() {\r\n    if (params.produto === 'picpay') {\r\n      return (\r\n        <p className={style.pSaibaMais}>\r\n          {'Quer saber mais sobre a conta ' +\r\n            produto +\r\n            ', confira detalhadamente em nosso blog. '}\r\n          <a\r\n            href=\"https://blog.guardardinheiro.com.br/como-guardar-dinheiro-no-PicPay\"\r\n            target=\"_blank\"\r\n          >\r\n            Acesse\r\n          </a>\r\n        </p>\r\n      );\r\n    } else if (params.produto === 'mercadopago') {\r\n      return (\r\n        <p className={style.pSaibaMais}>\r\n          {'Quer saber mais sobre a conta ' +\r\n            produto +\r\n            ', confira detalhadamente em nosso blog. '}\r\n          <a\r\n            href=\"https://blog.guardardinheiro.com.br/como-guardar-dinheiro-no-mercado-pago/\"\r\n            target=\"_blank\"\r\n          >\r\n            Acesse\r\n          </a>\r\n        </p>\r\n      );\r\n    } else if (params.produto === 'nubank') {\r\n      return (\r\n        <p className={style.pSaibaMais}>\r\n          {'Quer saber mais sobre a conta ' +\r\n            produto +\r\n            ', confira detalhadamente em nosso blog. '}\r\n          <a\r\n            href=\"https://blog.guardardinheiro.com.br/como-guardar-dinheiro-no-nubank/\"\r\n            target=\"_blank\"\r\n          >\r\n            Acesse\r\n          </a>\r\n        </p>\r\n      );\r\n    } else {\r\n      return null;\r\n    }\r\n  }\r\n\r\n  function retornaBotaoProduto() {\r\n    if (\r\n      params.produto === 'picpay' ||\r\n      params.produto === 'mercadopago' ||\r\n      params.produto === 'pagbank' ||\r\n      params.produto === 'next' ||\r\n      params.produto === 'poupanca' ||\r\n      params.produto === 'tesouro' ||\r\n      params.produto === 'inter' ||\r\n      params.produto === '99pay' ||\r\n      params.produto === 'neon' ||\r\n      params.produto === 'iti' ||\r\n      params.produto === 'will' ||\r\n      params.produto === 'fgts' ||\r\n      params.produto === 'nubank'\r\n    ) {\r\n      return (\r\n        <div className={style.divBotao}>\r\n          <div className={style.divBotao2}>\r\n            <button\r\n              id={'btn_' + params.produto}\r\n              className={style.btn}\r\n              onClick={handleClickAbrirConta}\r\n            >\r\n              {retornaTextoBotao()}\r\n            </button>\r\n\r\n            {params.produto === 'tesouro' ? (\r\n              <p className={style.p}>\r\n                Comece a investir em Tesouro Direto <br></br>no PagBank com\r\n                apenas 30 reais.\r\n              </p>\r\n            ) : null}\r\n            {params.produto === 'poupanca' ? (\r\n              <p className={style.p}>\r\n                Comece a investir no PagBank com apenas 30 reais e faa seu\r\n                dinheiro render o dobro da poupana.\r\n              </p>\r\n            ) : null}\r\n          </div>\r\n        </div>\r\n      );\r\n    } else return null;\r\n  }\r\n\r\n  function retornaTextoBotao() {\r\n    if (params.produto === 'picpay') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o PicPay';\r\n      // return 'Abrir Conta PicPay';\r\n    } else if (params.produto === 'mercadopago') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o MercadoPago';\r\n      // return 'Abrir Conta MercadoPago';\r\n    } else if (params.produto === 'pagbank') {\r\n      return 'Abrir Conta no PagBank';\r\n    } else if (params.produto === 'next') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o Banco Next';\r\n      // return 'Abrir Conta no Banco Next';\r\n    } else if (params.produto === 'poupanca') {\r\n      return 'Abrir Conta no PagBank';\r\n    } else if (params.produto === 'tesouro') {\r\n      return 'Abrir Conta no Pagbank';\r\n    } else if (params.produto === 'nubank') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o Nubank';\r\n      // return 'Abrir Conta no Nubank';\r\n    } else if (params.produto === 'inter') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o Banco Inter';\r\n      // return 'Abrir Conta no Nubank';\r\n    } else if (params.produto === '99pay') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o 99Pay';\r\n    } else if (params.produto === 'neon') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o Banco Neon';\r\n    } else if (params.produto === 'iti') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o Iti';\r\n    } else if (params.produto === 'will') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o Will Bank';\r\n    } else if (params.produto === 'fgts') {\r\n      return 'Confira Aqui Contas que Rendem Mais que o FGTS';\r\n    }\r\n  }\r\n\r\n  function handleClickAbrirConta(event) {\r\n    event.preventDefault();\r\n    console.log(event.target.id);\r\n\r\n    if (event.target.id === 'btn_pagbank') {\r\n      window.open('https://bit.ly/calculadora-pagbank', '_blank');\r\n    } else if (event.target.id === 'btn_mercadopago') {\r\n      // window.open('http://bit.ly/gd_renda_fixa_mp', '_blank');\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_next') {\r\n      // window.open(\r\n      //   'https://next.me/mgm?n=R1JBQ1kgRkVSUkVJUkE=&h=nsvoajlk2n22zJQeM1xayA==',\r\n      //   '_blank',\r\n      // );\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_picpay') {\r\n      // window.open('http://bit.ly/gd_renda_fixa_pp', '_blank');\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_nubank') {\r\n      // window.open('http://bit.ly/gd_renda_fixa_pp', '_blank');\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_inter') {\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_99pay') {\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_neon') {\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_iti') {\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (event.target.id === 'btn_fgts') {\r\n      window.open('/calculadora-renda-fixa', '_self');\r\n    } else if (\r\n      event.target.id === 'btn_tesouro' ||\r\n      event.target.id === 'btn_poupanca'\r\n    ) {\r\n      window.open('https://bit.ly/calculadora-pagbank', '_blank');\r\n      // window.open(\r\n      //   'https://v2.afilio.com.br/tracker.php?banid=4564988&campid=14984;3343&siteid=63081',\r\n      //   '_blank',\r\n      // );\r\n    }\r\n  }\r\n\r\n  function handleOver() {\r\n    if (!mobile) {\r\n      var divAdsense = document.getElementById('divAdsenseComparar');\r\n      divAdsense.style.display = 'flex';\r\n    }\r\n  }\r\n\r\n  function retornaLogoInstituicao(instituicao) {\r\n    if (instituicao === 'pagbank') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '35px', width: '110px' }}\r\n            src={fgPagBank}\r\n            alt=\"PagBank\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'tesouro') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '70px', width: '85px' }}\r\n            src={fgTesouro}\r\n            alt=\"Tesouro Selic\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'picpay') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '40px', width: '85px' }}\r\n            src={fgPicPay}\r\n            alt=\"PicPay\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'nubank') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '35px', width: '90px' }}\r\n            src={fgNuBank}\r\n            alt=\"NuBank\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'poupanca') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '70px', width: '75px' }}\r\n            src={fgPoupanca}\r\n            alt=\"Poupana\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'next') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          {!mobile ? (\r\n            <Image\r\n              // style={{ height: '150px', width: '165px' }}\r\n              src={fgBancoNext}\r\n              alt=\"Banco Next\"\r\n              fluid\r\n            />\r\n          ) : (\r\n            <Image src={fgBancoNext} alt=\"Banco Next\" fluid />\r\n          )}\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'mercadopago') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fgMercadoPago}\r\n            alt=\"Mercado Pago\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'inter') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fgInter}\r\n            alt=\"Banco Inter\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === '99pay') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fg99}\r\n            alt=\"Banco Inter\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'neon') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fgNeon}\r\n            alt=\"Banco Neon\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'iti') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fgIti}\r\n            alt=\"Iti\"\r\n            fluid\r\n          />\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'will') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fgWill}\r\n            alt=\"Will Bank\"\r\n            fluid\r\n          />\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'fgts') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '30px', width: '105px' }}\r\n            src={fgFgts}\r\n            alt=\"FGTS\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    }\r\n  }\r\n\r\n  function retornaTabela() {\r\n    return (\r\n      <div className={style.containerMaster}>\r\n        <div className={style.container}>\r\n          <div className={style.item}>\r\n            <div className={style.itemTitulo}>\r\n              <div className={style.detalheTitulo}>\r\n                {retornaLogoInstituicao(params.produto)}\r\n              </div>\r\n              <div className={style.detalheTitulo}>\r\n                <div className={style.divValores}>\r\n                  <span className={style.spanPositivo}>\r\n                    Investimento Inicial:\r\n                    {inputValor}\r\n                  </span>\r\n                </div>\r\n                <div className={style.divValores}>\r\n                  <span className={style.spanPositivo}>\r\n                    Investimento Mensal:\r\n                    {inputValorMensal}\r\n                  </span>\r\n                </div>\r\n                <div className={style.divValores}>\r\n                  <span className={style.spanPositivo}>\r\n                    Investimento Total em{' '}\r\n                    {tempo > 1 ? tempo + ' Meses: ' : tempo + ' Ms: '}\r\n                    {formatter.format(investimento)}\r\n                  </span>\r\n                </div>\r\n                <div className={style.divValores}>\r\n                  <span className={style.spanPositivo}>\r\n                    Valor Bruto:\r\n                    {formatter.format(retornoBruto)}\r\n                  </span>\r\n                </div>\r\n                <div className={style.divValores}>\r\n                  {' '}\r\n                  <span className={style.spanPositivo}>\r\n                    Valor Lquido:\r\n                    {formatter.format(retornoLiquido)}\r\n                  </span>\r\n                </div>\r\n                <div className={style.divValores}>\r\n                  <span className={style.spanNegativo}>\r\n                    Impostos:\r\n                    {formatter.format(imposto)}\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <Campanhas campanha={params.produto} />\r\n            {retornaBotaoProduto()}\r\n            <div>\r\n              <p className={style.pCalculadora}>\r\n                Confira tambm nossa Calculadora de Renda Fixa. Compare o\r\n                Rendimento entre Poupana, NuBank, PicPay, MarcadoPago, Pagbank,\r\n                CDB, Tesouro e muito mais.\r\n                <br></br>\r\n                <a href=\"/calculadora-renda-fixa\" rel=\"dofollow\">\r\n                  Clique Aqui para acessar a Calculadora\r\n                </a>\r\n              </p>\r\n            </div>\r\n          </div>\r\n          {!mobile ? (\r\n            <div className={style.itemAdsense}>\r\n              <AdsenseGrow nomeAdsense={'meia-pagina'} />\r\n            </div>\r\n          ) : null}\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <div className=\"App\">\r\n      <Head\r\n        title={getTitle()}\r\n        metaDescription={getTitle()}\r\n        h1={getTitle()}\r\n        canonical={\r\n          'https://www.guardardinheiro.com.br/quanto-rende/' +\r\n          params.valor +\r\n          '/' +\r\n          params.produto\r\n        }\r\n        nivel=\"2\"\r\n      />\r\n\r\n      <Container>\r\n        <Breadcrumb className={styles.breadcumb}>\r\n          <Breadcrumb.Item href=\"/\">\r\n            <FontAwesomeIcon icon={faHome} className={styles.spanBreadcumb} />\r\n            <span className={styles.spanBreadcumb}>{' Home'}</span>\r\n          </Breadcrumb.Item>\r\n          <Breadcrumb.Item active style={{ color: '#3F3F44' }}>\r\n            {getTitle()}\r\n          </Breadcrumb.Item>\r\n        </Breadcrumb>\r\n        <h1 className={styles.h1}>{getTitle()}</h1>\r\n        <h2 className={styles.h2}>\r\n          {'Simulador de Rendimento de ' +\r\n            produto +\r\n            ' para Guardar Dinheiro com a Selic a 13,75% ao ano'}\r\n        </h2>\r\n        {retornaURLBlog()}\r\n        <div id=\"divAdsenseComparar\" className={styles.divContainerAdsense}>\r\n          <AdsenseMiddlePage />\r\n        </div>\r\n\r\n        <form onSubmit={handleSubmit}>\r\n          <div className={stylesCalculadora.container}>\r\n            <div>\r\n              <InputMask\r\n                label=\"QUAL VALOR GOSTARIA DE INVESTIR?\"\r\n                placeholder=\"valor que gostaria de investir\"\r\n                value={inputValor}\r\n                ref={inputElement}\r\n                thousandSeparator={'.'}\r\n                decimalSeparator={','}\r\n                prefix={'R$'}\r\n                suffix={',00'}\r\n                onChange={({ target }) => setInputValor(target.value)}\r\n              ></InputMask>\r\n            </div>\r\n\r\n            <div>\r\n              <InputMask\r\n                label=\"QUANTO GOSTARIA DE INVESTIR POR MS?\"\r\n                placeholder=\"valor a investir mensalmente\"\r\n                value={inputValorMensal}\r\n                ref={inputElement}\r\n                thousandSeparator={'.'}\r\n                decimalSeparator={','}\r\n                prefix={'R$'}\r\n                suffix={',00'}\r\n                onChange={({ target }) => setInputValorMensal(target.value)}\r\n              ></InputMask>\r\n            </div>\r\n            <div>\r\n              <Input\r\n                label=\"QUANTOS MESES DEIXARIA ESSE DINHEIRO INVESTIDO?\"\r\n                type=\"number\"\r\n                placeholder=\"quantidade de meses\"\r\n                value={inputPeriodo}\r\n                ref={inputElement}\r\n                onChange={({ target }) => setInputPeriodo(target.value)}\r\n              ></Input>\r\n            </div>\r\n\r\n            <div>\r\n              {loading ? (\r\n                <button\r\n                  className={[\r\n                    stylesCalculadora.buttonCalcular,\r\n                    stylesCal