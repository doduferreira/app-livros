ata.map((rendaFixa) => (\r\n              <div className={stylesResultados.itemMobile}>\r\n                <table>\r\n                  {/* <tr>\r\n          <td colSpan=\"2\">{rendaFixa.produto}</td>\r\n        </tr> */}\r\n                  <tr>\r\n                    <td>{retornaLogoInstituicao(rendaFixa.produto)}</td>\r\n                    <td>\r\n                      <ul>\r\n                        <p className={stylesResultados.paragrafo}>\r\n                          {retornaNomeProduto(rendaFixa.produto)}\r\n                        </p>\r\n                        <li className={stylesResultados.li}>\r\n                          <b>\r\n                            {parseInt(rendaFixa.ranking) + 1}º {'Lugar'}\r\n                          </b>\r\n                        </li>\r\n                        <li className={stylesResultados.li}>\r\n                          {' '}\r\n                          <b>Retorno:</b> {rendaFixa.rentabilidade}\r\n                          {retornaRentabilidade(rendaFixa.produto)}\r\n                        </li>\r\n                        <li className={stylesResultados.li}>\r\n                          <b>Valor bruto:</b>\r\n                          {formatter.format(rendaFixa.valor_bruto)}\r\n                        </li>\r\n                        <li className={stylesResultados.li}>\r\n                          <b>Valor Líquido:</b>\r\n                          {formatter.format(rendaFixa.valor_liquido)}\r\n                        </li>\r\n                        <li className={stylesResultados.li}>\r\n                          <b>Impostos:</b>\r\n                          {formatter.format(rendaFixa.valor_ir)}\r\n                        </li>\r\n                        <li className={stylesResultados.li}>\r\n                          <Button\r\n                            id={rendaFixa.produto}\r\n                            className={stylesButton.buttonAbrirConta}\r\n                            onClick={handleClickAbrirConta}\r\n                          >\r\n                            Abrir Conta\r\n                          </Button>\r\n                        </li>\r\n                      </ul>\r\n                    </td>\r\n                  </tr>\r\n                </table>\r\n              </div>\r\n            ))}\r\n          </div>\r\n        </Container>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default RendaFixaResultados;\r\n",["1046","1047"],"C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Components\\Helper\\Error.js",[],["1048","1049"],"C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Components\\RendaFixa\\ComoJuntar.js",["1050","1051","1052"],"// import { faSearch } from '@fortawesome/free-solid-svg-icons';\r\nimport { useParams, useNavigate } from 'react-router-dom';\r\n// import { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\r\nimport React from 'react';\r\nimport { Spinner, Container } from 'react-bootstrap';\r\nimport styles from '../StylesGlobal/StylesGlobal.module.css';\r\nimport style from './QuantoRende.module.css';\r\nimport stylesCalculadora from './CalculadoraRendaFixa3.module.css';\r\nimport stylesJuntar from './ComoJuntar.module.css';\r\n// import Button from '../Forms/Button';\r\n// import stylesButton from '../StylesGlobal/Button.module.css';\r\nimport Input from './../Forms/Input';\r\nimport InputMask from './../Forms/InputMask';\r\nimport isMobile from '../Helper/Mobile';\r\nimport { Helmet } from 'react-helmet';\r\nimport { Image } from 'react-bootstrap';\r\nimport CountUp from 'react-countup';\r\n// import AdsenseMiddlePage from '../Adsense/AdsenseMiddlePage';\r\n// import AdsenseResponsivo from '../Adsense/AdsenseResponsivo';\r\nimport fgPagBank from '../../Assets/pagbank.png';\r\nimport fgTesouro from '../../Assets/tesouro.png';\r\nimport fgPicPay from '../../Assets/picpay.png';\r\nimport fgBancoNext from '../../Assets/banconext.png';\r\nimport fgNuBank from '../../Assets/nubank.png';\r\nimport fgNeon from '../../Assets/neon.png';\r\nimport fgPoupanca from '../../Assets/poupanca.png';\r\nimport fgMercadoPago from '../../Assets/mercadopago.png';\r\nimport fgInter from '../../Assets/inter.png';\r\nimport fg99 from '../../Assets/99pay.png';\r\nimport { faHome } from '@fortawesome/free-solid-svg-icons';\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\r\nimport Breadcrumb from 'react-bootstrap/Breadcrumb';\r\n// import AdsenseGrow from '../Adsense/AdsenseGrow';\r\nimport AdsenseFixoProdutos from '../Adsense/AdsenseFixoProdutos';\r\n\r\nconst ComoJuntar = () => {\r\n  const params = useParams();\r\n  const navigate = useNavigate();\r\n\r\n  const [inputValorInicial, setInputValorInicial] = React.useState(null);\r\n  const [inputValorMeta, setInputValorMeta] = React.useState(\r\n    params.valor > 0 ? params.valor : 1000000,\r\n  );\r\n  const [inputValorMensal, setInputValorMensal] = React.useState(\r\n    params.valor >= 100000 ? 1000 : 100,\r\n  );\r\n  const [inputValorTaxa, setInputValorTaxa] = React.useState(13.25);\r\n  // const [valorTotal, setValorTotal] = React.useState(null);\r\n  const [listaDados, setListaDados] = React.useState([]);\r\n\r\n  const [frase, setFrase] = React.useState(null);\r\n\r\n  const inputElement = React.useRef();\r\n  const mobile = isMobile();\r\n\r\n  const loading = false;\r\n\r\n  const formatter = new Intl.NumberFormat('pt-BR', {\r\n    style: 'currency',\r\n    currency: 'BRL',\r\n    minimumFractionDigits: 2,\r\n  });\r\n\r\n  React.useEffect(() => {\r\n    // retornaResultados(params.valor, 12);\r\n    // window.scroll(0, 0);\r\n    // getNomeProduto();\r\n    if (params.valor > 0) {\r\n      calculaJurosCompostos(0, inputValorTaxa, inputValorMensal, params.valor);\r\n    }\r\n  }, []);\r\n\r\n  function calculaJurosCompostos(capitalInicial, taxa, valorMensal, valorMeta) {\r\n    taxa = parseFloat(taxa.toString().replace(',', '.'));\r\n    if (!capitalInicial) {\r\n      capitalInicial = 0;\r\n    }\r\n\r\n    if (!valorMensal) {\r\n      valorMensal = 0;\r\n    }\r\n\r\n    let taxaMensal = taxa / 12;\r\n    let tempoFinal = 0;\r\n    let montante = parseFloat(capitalInicial);\r\n    let evolucao;\r\n    let listaEvoluacao = [];\r\n    let totalInvestido = parseFloat(capitalInicial);\r\n    while (montante < valorMeta) {\r\n      // console.log(montante, valorMeta);\r\n      totalInvestido += parseFloat(valorMensal);\r\n      montante += parseFloat(valorMensal);\r\n      montante += (parseFloat(montante) * parseFloat(taxaMensal)) / 100;\r\n      tempoFinal += 1;\r\n      evolucao = {\r\n        mes: tempoFinal,\r\n        totalInvestido: parseFloat(totalInvestido),\r\n        valorMensal: parseFloat(valorMensal),\r\n        montante: parseFloat(montante),\r\n      };\r\n      listaEvoluacao.push(evolucao);\r\n      setListaDados(listaEvoluacao);\r\n    }\r\n\r\n    let anos = (tempoFinal * 30) / 360;\r\n    let meses = ((tempoFinal * 30) % 360) / 30;\r\n\r\n    if ((anos > 1) & (meses >= 1)) {\r\n      setFrase(\r\n        'Com uma taxa de retorno de' +\r\n          formatter.format(inputValorTaxa).replace('R$', '') +\r\n          '% ao ano, você levará ' +\r\n          Math.trunc(anos) +\r\n          ' anos e ' +\r\n          Math.trunc(meses) +\r\n          ' meses para juntar um montante de ' +\r\n          formatter.format(inputValorMeta) +\r\n          ', investindo ' +\r\n          formatter.format(inputValorMensal) +\r\n          ' por mês.',\r\n      );\r\n    } else if (anos < 1) {\r\n      setFrase(\r\n        'Com uma taxa de retorno de' +\r\n          formatter.format(inputValorTaxa).replace('R$', '') +\r\n          '% ao ano, você levará ' +\r\n          Math.trunc(meses) +\r\n          ' meses para juntar um montante de ' +\r\n          formatter.format(inputValorMeta) +\r\n          ', investindo ' +\r\n          formatter.format(inputValorMensal) +\r\n          ' por mês.',\r\n      );\r\n    } else {\r\n      setFrase(\r\n        'Com uma taxa de retorno de' +\r\n          formatter.format(inputValorTaxa).replace('R$', '') +\r\n          '% ao ano, você levará ' +\r\n          Math.trunc(anos) +\r\n          ' anos para juntar um montante de ' +\r\n          formatter.format(inputValorMeta) +\r\n          ', investindo ' +\r\n          formatter.format(inputValorMensal) +\r\n          ' por mês.',\r\n      );\r\n\r\n      let a = document.getElementById('resultado');\r\n\r\n      if (a) {\r\n        a.scrollIntoView({ behavior: 'smooth', block: 'center' });\r\n      }\r\n    }\r\n\r\n    return (\r\n      <div className={styles.stylesJuntar}>\r\n        <div></div>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  function handleSubmit(event) {\r\n    event.preventDefault();\r\n\r\n    calculaJurosCompostos(\r\n      inputValorInicial,\r\n      inputValorTaxa,\r\n      inputValorMensal,\r\n      inputValorMeta,\r\n    );\r\n  }\r\n\r\n  function getTitle() {\r\n    let titulo = '';\r\n\r\n    if (params.valor > 0) {\r\n      titulo = 'Como juntar R$ ' + parseFloat(params.valor).toFixed(2);\r\n    } else {\r\n      titulo = 'Calculadora do Milhão';\r\n    }\r\n\r\n    return titulo.replace('.', ',');\r\n  }\r\n\r\n  function retornaShemaOrg() {\r\n    var endereco =\r\n      'https://www.guardardinheiro.com.br/como-juntar/' + params.valor;\r\n    return (\r\n      <script type=\"application/ld+json\">\r\n        {`{\r\n            \"@context\":\"https://schema.org\",\r\n            \"@type\":\"WebPage\",\r\n            \"@id\": \"${endereco}#webpage\",\r\n            \"url\":\"${endereco}\",\r\n            \"name\":\"${getTitle()}\",\r\n            \"isPartOf\":\r\n              {\r\n                \"@id\":\"https://www.guardardinheiro.com.br/#website\"\r\n                },\r\n            \"description\":\"${getTitle()}\",\r\n            \"inLanguage\":\"pt-BR\"\r\n          }`}\r\n      </script>\r\n    );\r\n  }\r\n\r\n  function retornaSchemaBreadcrumbList() {\r\n    var endereco =\r\n      'https://www.guardardinheiro.com.br/como-juntar/' + params.valor;\r\n\r\n    return (\r\n      <script type=\"application/ld+json\">\r\n        {`{\r\n          \"@context\":\"https://schema.org\",\r\n          \"@type\":\"BreadcrumbList\",\r\n          \"itemListElement\":\r\n            [\r\n              {\r\n                \"@type\":\"ListItem\",\r\n                \"position\":1,\r\n                \"name\":\"Guardar Dinheiro\",\r\n                \"item\":\"https://www.guardardinheiro.com.br\"\r\n              },\r\n              {\r\n                \"@type\":\"ListItem\",\r\n                \"position\":2,\r\n                \"name\":\"${getTitle()}\",\r\n                \"item\":\"${endereco}\"\r\n              }              \r\n            ]\r\n        }`}\r\n      </script>\r\n    );\r\n  }\r\n\r\n  function retornaBotaoProduto() {\r\n    if (\r\n      params.produto === 'picpay' ||\r\n      params.produto === 'mercadopago' ||\r\n      params.produto === 'pagbank' ||\r\n      params.produto === 'next' ||\r\n      params.produto === 'poupanca' ||\r\n      params.produto === 'tesouro' ||\r\n      params.produto === 'inter' ||\r\n      params.produto === '99pay' ||\r\n      params.produto === 'neon' ||\r\n      params.produto === 'nubank'\r\n    ) {\r\n      return (\r\n        <div className={style.divBotao}>\r\n          <div className={style.divBotao2}>\r\n            <button\r\n              id={'btn_' + params.produto}\r\n              className={style.btn}\r\n              onClick={handleClickAbrirConta}\r\n            >\r\n              {retornaTextoBotao()}\r\n            </button>\r\n            {params.produto === 'tesouro' ? (\r\n              <p className={style.p}>\r\n                Comece a investir em Tesouro Direto <br></br>no PagBank com\r\n                apenas 30 reais.\r\n              </p>\r\n            ) : null}\r\n            {params.produto === 'poupanca' ? (\r\n              <p className={style.p}>\r\n   