     } else {\r\n            sorted = [...dados].sort(\r\n              (a, b) => a[sortProperty] - b[sortProperty],\r\n            );\r\n          }\r\n        } else {\r\n          sorted = [...data];\r\n\r\n          if (type.includes('maior')) {\r\n            sorted = [...data].sort(\r\n              (a, b) => b[sortProperty] - a[sortProperty],\r\n            );\r\n          } else {\r\n            sorted = [...data].sort(\r\n              (a, b) => a[sortProperty] - b[sortProperty],\r\n            );\r\n          }\r\n        }\r\n\r\n        setDados(sorted);\r\n      };\r\n\r\n      sortArray(sortType);\r\n    } else {\r\n      window.scroll(0, 0);\r\n\r\n      // const { url, options } = AWS_GET_INVESTIMENTOS('FUNDOS', 'TODOS');\r\n      const { url, options } = AWS_GET_INVESTIMENTOS_GZIP();\r\n      request(url, options);\r\n    }\r\n  }, [request, sortType]);\r\n\r\n  function handleChange({ target }) {\r\n    if (target.id.includes('chk_instituicao')) {\r\n      if (target.checked) {\r\n        setInstituicoes([...instituicoes, target.value]);\r\n      } else {\r\n        setInstituicoes(\r\n          instituicoes.filter((instituicao) => instituicao !== target.value),\r\n        );\r\n      }\r\n    } else if (target.id.includes('chk_classe')) {\r\n      if (target.checked) {\r\n        setClasses([...classes, target.value]);\r\n      } else {\r\n        setClasses(classes.filter((classe) => classe !== target.value));\r\n      }\r\n    } else if (target.id.includes('chk_risco')) {\r\n      if (target.checked) {\r\n        setRiscos([...riscos, target.value]);\r\n      } else {\r\n        setRiscos(riscos.filter((risco) => risco !== target.value));\r\n      }\r\n    }\r\n  }\r\n\r\n  function handleCheckedInstituicao(instituicao) {\r\n    return instituicoes.includes(instituicao);\r\n  }\r\n\r\n  function handleCheckedClasse(classe) {\r\n    return classes.includes(classe);\r\n  }\r\n\r\n  function handleCheckedRisco(risco) {\r\n    // console.log(risco);\r\n    return riscos.includes(risco);\r\n  }\r\n\r\n  function handleSubmit(event) {\r\n    event.preventDefault();\r\n    const btnFiltros = document.getElementById('btn-filtros');\r\n    // console.log(btnFiltros.innerText);\r\n    if (btnFiltros.innerText === 'FILTROS +') {\r\n      setFiltro(true);\r\n      btnFiltros.innerText = 'FILTROS -';\r\n    } else {\r\n      setFiltro(false);\r\n      btnFiltros.innerText = 'FILTROS +';\r\n    }\r\n    // console.log(btnFiltros);\r\n  }\r\n\r\n  function hanfleClickPesquisar(event) {\r\n    event.preventDefault();\r\n    setFiltroUtilizado(true);\r\n\r\n    setRentabilidade('Selecione');\r\n\r\n    let dados3 = data;\r\n\r\n    if (classes.length > 1) {\r\n      dados3 = dados3.filter((registro) => classes?.includes(registro.classe));\r\n    }\r\n\r\n    if (instituicoes.length > 1) {\r\n      dados3 = dados3.filter((registro) =>\r\n        instituicoes.some((r) => registro.instituicao.split('|').includes(r)),\r\n      );\r\n    }\r\n\r\n    if (riscos.length > 1) {\r\n      dados3 = dados3.filter((registro) => riscos?.includes(registro.risco));\r\n    }\r\n\r\n    if (aplicacaoInicial.value !== '') {\r\n      let aplicacao = parseInt(aplicacaoInicial.value);\r\n      dados3 = dados3.filter(\r\n        (registro) =>\r\n          parseInt(registro.aplicacao_inicial.replace('.', '')) < aplicacao,\r\n      );\r\n    }\r\n\r\n    if (\r\n      aplicacaoInicial.value === '' &&\r\n      instituicoes.length === 1 &&\r\n      classes.length === 1 &&\r\n      riscos.length === 1\r\n    ) {\r\n      setFiltroUtilizado(false);\r\n      setDados(data);\r\n    } else {\r\n      setDados(dados3);\r\n    }\r\n    setFiltro(false);\r\n  }\r\n\r\n  function retornaListaOrdenacao() {\r\n    return (\r\n      <select\r\n        className={style.dropdownList}\r\n        id=\"seletorOrdenacao\"\r\n        onChange={(e) => setSortType(e.target.value)}\r\n        value={rentabilidade}\r\n      >\r\n        <option className={style.dropdownList} key=\"Ordenar:\" value=\"Ordenar\">\r\n          ORDENAR:\r\n        </option>\r\n        {dataRentabilidade.map((rentabilidade) => (\r\n          <option\r\n            className={style.dropdownList}\r\n            key={rentabilidade}\r\n            value={rentabilidade}\r\n          >\r\n            {/* {handleStrings('ordenacao', rentabilidade)} */}\r\n            {formataListaOrdenacao(rentabilidade)}\r\n          </option>\r\n        ))}\r\n      </select>\r\n    );\r\n  }\r\n\r\n  function retornaDados() {\r\n    if (!dados) {\r\n      return (\r\n        <React.Suspense fallback={<div></div>}>\r\n          <FundosLista data={data} />\r\n        </React.Suspense>\r\n      );\r\n    } else {\r\n      return (\r\n        <React.Suspense fallback={<div></div>}>\r\n          <FundosLista data={dados} />\r\n        </React.Suspense>\r\n      );\r\n    }\r\n  }\r\n\r\n  function postFundos() {\r\n    return (\r\n      <div>\r\n        <Paragrafo\r\n          texto={\r\n            'Compare os Melhores Fundos de Investimentos de 2023 das principais instituies financeiras (XP, BTG, Nubank, Ita e mais) em uma nica tela gratuitamente.'\r\n          }\r\n        />\r\n        <Paragrafo\r\n          texto={\r\n            'Fundos de Investimentos tem sido atualmente um dos investimentos mais procurados pelos investidores, e por conta disso, criamos uma ferramenta gratuita com informaes dos Melhores Fundos de Investimentos de 2023.'\r\n          }\r\n        />\r\n        <AdsenseParagrafo1 />\r\n        <Paragrafo\r\n          texto={\r\n            'Nossa Ferramenta de Melhores Fundos de Investimentos de 2023,  uma das ferramentas mais elogiadas pelas pessoas que utilizam nosso site.'\r\n          }\r\n        />\r\n\r\n        <Paragrafo\r\n          texto={\r\n            'Com nossa ferramenta voc consegue fazer estudos, comparaes e tomar decises rapidamente sem perder muito tempo, pois trazemos comparaes das melhores instituies do mercado, ordenado pelos melhores fundos de investimentos de 2023.'\r\n          }\r\n        />\r\n        <AdsenseParagrafo3 />\r\n\r\n        <H2 texto=\"Qual  o melhor fundo de investimento?\" />\r\n        <Paragrafo\r\n          texto={\r\n            'Estamos trazendo todos os fundos de investimento de cada instituio abaixo listada, para que voc possa analisar os Melhores Fundos de Investimentos sem ter que acessar o site de cada uma:'\r\n          }\r\n        />\r\n        <ul className={styleLista.checkList}>\r\n          <li>XP Investimentos</li>\r\n          <li>BTG Pactual</li>\r\n          <li>Ativa</li>\r\n          <li>Banco do Brasil</li>\r\n          <li>Bradesco</li>\r\n          <li>C6 Bank</li>\r\n          <li>Genial Investimentos</li>\r\n          <li>Guide Investimentos</li>\r\n          <li>Banco Inter</li>\r\n          <li>Ita</li>\r\n          <li>Mirae</li>\r\n          <li>Modal Mais</li>\r\n          <li>NuInvest</li>\r\n          <li>Orama</li>\r\n          <li>PagBank</li>\r\n          <li>Safra</li>\r\n          <li>Vitreo</li>\r\n          <li>Warren</li>\r\n        </ul>\r\n\r\n        <H2 texto=\"Como Utilizar a Ferramenta Melhores Fundos de Investimentos 2023\" />\r\n        <Paragrafo\r\n          texto={\r\n            'Para comear, confira as informaes que disponibilizamos para te auxiliar:'\r\n          }\r\n        />\r\n        <ul className={styleLista.checkList}>\r\n          <li>Nome do fundo de investimento</li>\r\n          <li>Instituies onde o fundo de investimento est disponvel</li>\r\n          <li>Classe - (Renda Fixa/ Multimercado / Aes/ Cambial)</li>\r\n          <li>Risco - (Baixo/ Mdio Baixo/ Mdio/ Mdio Alto/ Alto)</li>\r\n          <li>Taxa de Administrao</li>\r\n          <li>Prazo de Resgate</li>\r\n          <li>Aplicao Mnima Inicial</li>\r\n          <li>Patrimnio Lquido</li>\r\n          <li>\r\n            Investidores no Ms - Quantidade de Investidores Novos que\r\n            investiram no fundo de investimento no ms atual\r\n          </li>\r\n          <li>Investidores Total</li>\r\n          <li>Rentabilidade Ms, Ano, 12 Meses</li>\r\n          <li>Rentabilidade Ms a Ms</li>\r\n          <li>Grficos de Evoluo de Rentabilidade</li>\r\n          <li>Comparaes com CDI e Ibovespa</li>\r\n          <li>\r\n            Simulao de Investimento se voc investisse neste fundo de\r\n            investimento\r\n          </li>\r\n        </ul>\r\n\r\n        <Paragrafo\r\n          texto={'Agora para utiliz-la, basta seguir os seguintes passos:'}\r\n        />\r\n        <ul className={styleLista.checkList}>\r\n          <li>\r\n            Utilize a Ordenao com o Ranking de Maiores Retornos no Ms, Ano e\r\n            12 Meses.\r\n          </li>\r\n          <li>\r\n            Utilize a Ordenao com o Ranking de Entrada de Investidores no Ms.\r\n            Aqui voc consegue saber a quantidade de investidores novos que\r\n            fizeram uma aplicao no fundo de investimento ou a quantidade de\r\n            investidores que resgataram todo seu dinheiro no fundo de\r\n            investimento.\r\n          </li>\r\n          <li>\r\n            Utilize o Filtro por Instituio. Filtre por exemplo, somente as\r\n            instituies que voc possui conta.\r\n          </li>\r\n          <li>\r\n            Utilize o Filtro por Risco - (Baixo/ Mdio Baixo/ Mdio/ Mdio Alto/\r\n            Alto).\r\n          </li>\r\n          <li>\r\n            Utilize o Filtro por Classe - (Renda Fixa / Multimercado / Aes /\r\n            Cambial).\r\n          </li>\r\n          <li>\r\n            Clique no nome do fundo de investimento e veja todas as informaes\r\n            detalhadas conforme lista de informaes acima.\r\n          </li>\r\n        </ul>\r\n        <Paragrafo texto=\"Agora voc j conhece como funciona e como utilizar a ferramenta Melhores Fundos de Investimentos 2023. Se precisar de ajuda  s entrar em contato com a gente. Bons Investimentos!\" />\r\n\r\n        <AdsenseParagrafo5 />\r\n      </div>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <Head\r\n        title=\"Melhores Fundos de Investimentos 2023 | Guardar Dinheiro\"\r\n        metaDescription=\"Compare os Melhores Fundos de Investimentos de 2023 das principais instituies financeiras (XP, BTG, Nubank, Ita e mais) em uma nica tela gratuitamente.\"\r\n        canonical=\"https://www.guardardinheiro.com.br/fundos-de-investimento\"\r\n        h1=\"Melhores Fundos de Investimentos 2023\"\r\n        nivel=\"2\"\r\n      />\r\n\r\n      <BreadcumbH1\r\n        paginaAtual={'Melhores Fundos de Investimentos 2023'}\r\n        texto={'Melhores Fundos de Investimentos 2023'}\r\n      />\r\n\r\n      <Container>\r\n        <div className={stylePost.container}>\r\n          <div className={stylePost.itemPost}>\r\n            {postFundos()}\r\n            <br />\r\n            <div className={style.containerMaster}>\r\n              <H2 texto={'Melhores Fundos de Investimentos 2023'} />\r\n              <div className={style.containerFiltros}>\r\n                <div className={style.itemFiltros}>\r\n                  <form onSubmit={handleSubmit}>\r\n                    <Button id=\"btn-filtros\" className={style.button}>\r\n                      FILTROS +\r\n                    </Button>\r\n\r\n                    {filtro && (\r\n                      <div className={style.form}>\r\n                        <label className={style.labelForm}>Corretora</label>\r\n                        <div className={style.containerForm}>\r\n                          {dataInsituicoes.map((instituicao) => (\r\n                            <div className={style.itemForm}>\r\n                              <label key={instituicao}>\r\n                                <input\r\n                                  id={'chk_instituicao_' + instituicao}\r\n                                  type=\"checkbox\"\r\n                                  value={instituicao}\r\n                                  checked={handleCheckedInstituicao(\r\n                                    instituicao,\r\n                                  )}\r\n                                  onChange={handleChange}\r\n                                />\r\n                                <span className={style.spanCheckList}>\r\n                                  {/* {handleStrings('corretora', instituicao)} */}\r\n                                  {formataCorretora(instituicao)}\r\n                                </span>\r\n                              </label>\r\n                            </div>\r\n                          ))}\r\n                        </div>\r\n                        <hr />\r\n                        <label className={style.labelForm}>Classe</label>\r\n                        <div className={style.containerForm}>\r\n                          {dataClasse.map((classe) => (\r\n                            <div className={style.itemForm}>\r\n                              <label key={classe}>\r\n                                <input\r\n                                  id={'chk_classe_' + classe}\r\n                                  type=\"checkbox\"\r\n                                  value={classe}\r\n                                  checked={handleCheckedClasse(classe)}\r\n                                  onChange={handleChange}\r\n                                  // onChange={(e) => setSortType(e.target.value)}\r\n                                />\r\n                                <span className={style.spanCheckList}>\r\n                                  {/* {handleStrings('classe', classe)} */}\r\n                                  {formataClasse(classe)}\r\n                                </span>\r\n                              </label>\r\n                            </div>\r\n                          ))}\r\n                        </div>\r\n                        <hr />\r\n                        <label className={style.labelForm}>Risco</label>\r\n                        <div className={s