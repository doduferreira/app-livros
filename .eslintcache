.target.id === 'btn_tesouro' ||\r\n      event.target.id === 'btn_poupanca'\r\n    ) {\r\n      window.open('https://bit.ly/calculadora-pagbank', '_blank');\r\n      // window.open(\r\n      //   'https://v2.afilio.com.br/tracker.php?banid=4564988&campid=14984;3343&siteid=63081',\r\n      //   '_blank',\r\n      // );\r\n    }\r\n  }\r\n\r\n  function retornaLogoInstituicao(instituicao) {\r\n    if (instituicao === 'pagbank') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '35px', width: '110px' }}\r\n            src={fgPagBank}\r\n            alt=\"PagBank\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'tesouro') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '70px', width: '85px' }}\r\n            src={fgTesouro}\r\n            alt=\"Tesouro Selic\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'picpay') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '40px', width: '85px' }}\r\n            src={fgPicPay}\r\n            alt=\"PicPay\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'nubank') {\r\n      return (\r\n        <div className={styles.tooltip}>\r\n          <Image\r\n            // style={{ height: '35px', width: '90px' }}\r\n            src={fgNuBank}\r\n            alt=\"NuBank\"\r\n            fluid\r\n          />\r\n          {/* <span className={styles.tooltiptext}>Banco Ita</span> */}\r\n        </div>\r\n      );\r\n    } else if (instituicao === 'poupanc