rno_ano' },\r\n      { Header: '%12Mes', accessor: 'retorno_12mes' },\r\n      { Header: 'Setor', accessor: 'setor' },\r\n    ],\r\n    [],\r\n  );\r\n\r\n  const {\r\n    getTableProps,\r\n    getTableBodyProps,\r\n    headerGroups,\r\n    page,\r\n    nextPage,\r\n    previousPage,\r\n    canPreviousPage,\r\n    canNextPage,\r\n    pageOptions,\r\n    state,\r\n    prepareRow,\r\n    setGlobalFilter,\r\n  } = useTable(\r\n    {\r\n      columns: mobile ? columns : columns,\r\n      data: data,\r\n      initialState: { pageSize: 30 },\r\n    },\r\n    useGlobalFilter,\r\n    useSortBy,\r\n    usePagination,\r\n  );\r\n\r\n  const { globalFilter } = state;\r\n\r\n  const { pageIndex } = state;\r\n\r\n  function handleClickAcoesDinamico(event) {\r\n    event.preventDefault();\r\n    // console.log(event.target.id);\r\n    // var str = event.target.id;\r\n    // var id = str.split('_')[0];\r\n    var acao = event.target.id;\r\n\r\n    if (isAcao) {\r\n      if (!mobile) {\r\n        // window.open('/acoes/' + acao.toLowerCase(), '_blank');\r\n        openInNewTab('/acoes/' + acao.toLowerCase());\r\n      } else {\r\n        navigate('/acoes/' + acao.toLowerCase());\r\n      }\r\n    } else {\r\n      if (!mobile) {\r\n        // window.open('/fii/' + acao.toLowerCase(), '_blank');\r\n        openInNewTab('/fii/' + acao.toLowerCase());\r\n      } else {\r\n        navigate('/fii/' + acao.toLowerCase());\r\n      }\r\n    }\r\n  }\r\n\r\n  function getRetorno(retorno) {\r\n    if (retorno >= 0) {\r\n      return (\r\n        <span className={styles.spanRetornoPositivo}>\r\n          {formatter.format(retorno).replace('R$', '')}\r\n        </span>\r\n      );\r\n    } else {\r\n      return (\r\n        <span className={styles.spanRetornoNegativo}>\r\n          {formatter.format(retorno).replace('R$', '')}\r\n        </span>\r\n      );\r\n    }\r\n  }\r\n\r\n  function showAdsense(linha) {\r\n    if (linha > 1) {\r\n      if (linha % 5 == 0) {\r\n        if (isAcao) {\r\n          return (\r\n            <Container>\r\n              <div>\r\n                {/* <br /> */}\r\n                {/* <AdsenseResponsivoDinamico nomeAdsense=\"site-resp-hor-acoes\" /> */}\r\n                <AdsenseFixoProdutos />\r\n                {/* <br /> */}\r\n              </div>\r\n            </Container>\r\n          );\r\n        } else {\r\n          return (\r\n            <Container>\r\n              <div>\r\n                {/* <br /> */}\r\n                {/* <AdsenseResponsivoDinamico nomeAdsense=\"site-resp-hor-fii\" /> */}\r\n                <AdsenseFixoProdutos />\r\n                {/* <br /> */}\r\n              </div>\r\n            </Container>\r\n          );\r\n        }\r\n      } else {\r\n        return null;\r\n      }\r\n    } else {\r\n      return null;\r\n    }\r\n  }\r\n\r\n  function handleClickFavoritos(event) {\r\n    var produto = 'FII';\r\n    if (isAcao) {\r\n      produto = 'ACAO';\r\n    }\r\n    // console.log(event.target.id);\r\n    if (login) {\r\n      var produtoChave = event.target.id.replace('btnFavoritos_', '');\r\n\r\n      addFavoritos('POST', produtoChave, produto);\r\n    } else {\r\n      window.scroll(0, 0);\r\n      navigate('/autenticacao');\r\n    }\r\n  }\r\n  function handleNextPage() {\r\n    let a = document.getElementById('resultados');\r\n\r\n    if (a) {\r\n      a.scrollIntoView();\r\n    }\r\n    nextPage();\r\n  }\r\n\r\n  function retornaTabela() {\r\n    return (\r\n      <div className={styles.container}>\r\n        <div id=\"resultados\" className={styles.item1}>\r\n          <GlobalFilter\r\n            filter={globalFilter}\r\n            setFilter={setGlobalFilter}\r\n            placeholder={' PESQUISE POR CDIGO, NOME OU SETOR'}\r\n          />\r\n          <div id=\"paginasheader\">\r\n            <span>\r\n              Pagina{' '}\r\n              <strong>\r\n                {pageIndex + 1} of {pageOptions.length}\r\n              </strong>{' '}\r\n            </span>\r\n  