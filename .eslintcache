           decimalSeparator={','}\r\n                        prefix={'R$'}\r\n                        suffix={',00'}\r\n                        value={inputValorEmprestimo}\r\n                        ref={inputElement}\r\n                        onChange={({ target }) =>\r\n                          setInputValorEmprestimo(\r\n                            target.value\r\n                              .replace('R$', '')\r\n                              .replaceAll('.', '')\r\n                              .replace(',00', ''),\r\n                          )\r\n                        }\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <div className={styles.subItem}>\r\n                    <div>\r\n                      <Input\r\n                        label=\"Em quantas parcelas gostaria de pagar?\"\r\n                        type=\"number\"\r\n                        placeholder=\"Exemplo:3 parcelas\"\r\n                        value={inputParcelas}\r\n                        ref={inputElement}\r\n                        onChange={({ target }) =>\r\n                          setInputParcelas(target.value)\r\n                        }\r\n                      ></Input>\r\n                    </div>\r\n                  </div>\r\n                  <div className={styles.subItem}>\r\n                    <div>\r\n                      <Input\r\n                        label=\"Qual a taxa de juros oferecida?\"\r\n                        type=\"number\"\r\n                        step={'0.01'}\r\n                        placeholder=\"%\"\r\n                        value={inputTaxa}\r\n                        ref={inputElement}\r\n                        onChange={({ target }) => setInputTaxa(target.value)}\r\n                      ></Input>\r\n\r\n                      <p className={styles.pTaxaMedia}>\r\n                        {'A Taxa Média das Instituições é ' +\r\n                          taxaMedia.toFixed(2).