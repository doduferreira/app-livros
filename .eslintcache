sk';\r\nimport { formataRetorno } from '../Helper/DateUtilities';\r\nimport { useParams } from 'react-router';\r\n\r\nconst Dividendos = (props) => {\r\n  const [inputValor, setInputValor] = React.useState(\r\n    props.valor > 0 ? props.valor : '100',\r\n  );\r\n  const inputElement = React.useRef();\r\n  const [possuiDividendos, setPossuiDividendos] = React.useState(false);\r\n\r\n  React.useEffect(() => {\r\n    if (\r\n      props.tipoInvestimento === 'acoes' ||\r\n      props.tipoInvestimento === 'fii'\r\n    ) {\r\n      setPossuiDividendos(true);\r\n    }\r\n  }, [props.tipoInvestimento]);\r\n\r\n  async function handleSubmit(event) {\r\n    event.preventDefault();\r\n    getResultado();\r\n  }\r\n\r\n  function getResultadoDividendos() {\r\n    console.log(inputValor);\r\n    let valor = parseFloat(\r\n      String(\r\n        inputValor.replace('R$', '').replaceAll('.', '').replace(',00', ''),\r\n      ),\r\n    );\r\n    let preco = props.data[1].preco_atual;\r\n    let cotas = valor / preco;\r\n    let dividendos = props.data[1].dividendo;\r\n    let dividendo_1_cotas = (parseFloat(preco) * parseFloat(dividendos)) / 100;\r\n    let dividendo_10_cotas = dividendo_1_cotas * 10;\r\n    let dividendo_100_cotas = dividendo_1_cotas * 100;\r\n    let dividendo_suas_cotas = dividendo_1_cotas * valor;\r\n    let msg_recebimento_dividendos = 'ano';\r\n\r\n    if (props.tipoInvestimento === 'fii') {\r\n      msg_recebimento_dividendos = 'ms';\r\n    }\r\n\r\n    return (\r\n      <div className={styles.divDividendos}>\r\n        <p className={styles.pDividendos}>\r\n          {'Se voc possuir ' +\r\n            valor +\r\n            ' cotas em ' +\r\n            props.acao.toUpperCase() +\r\n            ' com o preo atual de R$ ' +\r\n            formataRetorno(preco) +\r\n            ', de acordo com os ltimos dividendos de ' +\r\n            formataRetorno(props.data[1].dividendo) +\r\n            '% por cota, voc receberia por ' +\r\n            msg_recebimento_dividendos +\r\n            ' R$' +\r\n            formataRetorno(dividendo_suas_cotas) +\r\n            ' em dividendos.'}\r\n        </p>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  function getResultado() {\r\n    let indice = 1;\r\n    if (\r\n      props.tipoInvestimento === 'etf' ||\r\n      props.tipoInvestimento === 'cripto'\r\n    ) {\r\n      indice = 0;\r\n    } else if (props.tipoInvestimento === 'fundos') {\r\n      indice = 2;\r\n    }\r\n\r\n    console.log(inputValor);\r\n    let valor = parseFloat(\r\n      String(\r\n        inputValor.replace('R$', '').replaceAll('.', '').replace(',00', ''),\r\n      ),\r\n    );\r\n\r\n    let posicao_mes =\r\n      parseFloat(valor) +\r\n      parseFloat((props.data[indice].retorno_mes * valor) / 100);\r\n    let posicao_ano =\r\n      parseFloat(valor) +\r\n      parseFloat((props.data[indice].retorno_ano * valor) / 100);\r\n    let posicao_12mes =\r\n      parseFloat(valor) +\r\n      parseFloat((props.data[indice].retorno_12mes * valor) / 100);\r\n\r\n    return (\r\n      <div className={styles.divDividendos}>\r\n        <p className={styles.pDividendos}>\r\n          {'Confira quanto voc teria, se tivesse investido R$' +\r\n            formataRetorno(valor) +\r\n            ' no Ms, no Ano ou em 12 Meses.'}\r\n        </p>\r\n        <ul>\r\n          <li>\r\n            <p>{'Saldo no Ms : R$' + formataRetorno(posicao_mes)}</p>\r\n          </li>\r\n\r\n          <li>\r\n            <p>{'Saldo no Ano: R$' + formataRetorno(posicao_ano)}</p>\r\n          </li>\r\n          <li>\r\n            <p>{'Saldo em 12 Meses: R$' + formataRetorno(posicao_12mes)}</p>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <form className={styles.form} onSubmit={handleSubmit}>\r\n        <div className={styles.container}>\r\n          <div>\r\n            <InputMask\r\n              label={\r\n                (props.tipoInvestimento === 'fii') |\r\n                (props.tipoInvestimento === 'acoes')\r\n                  ? 'QUANTAS COTAS VOC POSSUI?'\r\n                  : 'QUAL VALOR GOSTARIA DE INVESTIR?'\r\n              }\r\n              placeholder={\r\n                (props.tipoInvestimento === 'fii') |\r\n                (props.tipoInvestimento === 'acoes')\r\n                  ? 'quantas cotas voc possui'\r\n                  : 'valor que gostaria de investir'\r\n              }\r\n              value={inputValor}\r\n              ref={inputElement}\r\n              thousandSeparator={'.'}\r\n              decimalSeparator={','}\r\n              prefix={\r\n                (props.tipoInvestimento === 'fii') |\r\n                (props.tipoInvestimento === 'acoes')\r\n                  ? ''\r\n                  : 'R$'\r\n              }\r\n              suffix={\r\n                (props.tipoInvestimento === 'fii') |\r\n                (props.tipoInvestimento === 'acoes')\r\n                  ? ''\r\n                  : ',00'\r\n              }\r\n              onChange={({ target }) => setInputValor(target.value)}\r\n            />\r\n          </div>\r\n\r\n          <div>\r\n            {possuiDividendos ? getResultadoDividendos() : getResultado()}\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default memo(Dividendos);\r\n","C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Pages\\Comparador\\Comparador.js",[],"C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Components\\Comparador\\ComparadorResultados.js",["1052","1053","1054","1055"],"import React from 'react';\r\nimport {\r\n  AWS_POST_COMPARADOR_FUNDOS_DF,\r\n  AWS_GET_INVESTIMENTOS_FILTRO,\r\n} from '../Integracoes/Api';\r\nimport useFetch from '../../Hooks/useFetch';\r\nimport Loading from '../Helper/Loading';\r\nimport styles from '../../Pages/Comparador/Comparador.module.css';\r\nimport RetornoAgrupado from '../Retornos/RetornoAgrupado';\r\nimport RetornoDetalhado from '../Retornos/RetornoDetalhado';\r\nimport RetornoEvolucao from '../Retornos/RetornoEvolucao';\r\nimport H2 from '../Tags/H2';\r\nimport Fundamentus2 from '../Retornos/Fundamentus2';\r\nimport AdsenseParagrafo1 from '../Adsense/AdsenseParagrafo1';\r\nimport AdsenseParagrafo3 from '../Adsense/AdsenseParagrafo3';\r\nimport AdsenseParagrafo5 from '../Adsense/AdsenseParagrafo5';\r\n\r\nconst ComparadorResultados = ({ acoesComparacao, tipo }) => {\r\n  const { data, loading, error, request } = useFetch();\r\n\r\n  React.useEffect(() => {\r\n    var d = new Date();\r\n    var hora = d.getHours();\r\n    var url = '';\r\n\r\n    if (tipo === 'fundos') {\r\n      const { url, options } = AWS_POST_COMPARADOR_FUNDOS_DF({\r\n        produtos: acoesComparacao,\r\n        tipo: 'fundos_anbima2',\r\n      });\r\n      request(url, options);\r\n    } else if (hora === 18 || hora === 19 || hora === 20) {\r\n      const { url, options } = AWS_POST_COMPARADOR_FUNDOS_DF({\r\n        produtos: '|' + acoesComparacao,\r\n        tipo: (tipo + '2').replace('bdr2', 'bdr'),\r\n      });\r\n      request(url, options);\r\n    } else {\r\n      const { url, options } = AWS_GET_INVESTIMENTOS_FILTRO({\r\n        produto: tipo.toUpperCase(),\r\n        chave: '|' + acoesComparacao.toUpperCase(),\r\n      });\r\n      request(url, options);\r\n    }\r\n  }, [request]);\r\n\r\n  if (loading) return <Loading />;\r\n  if (data)\r\n    return (\r\n      <>\r\n        <div className={styles.divInfo}>\r\n          <H2 texto={'RENTABILIDADE ACUMULADA'} />\r\n          <RetornoAgrupado data={data} tipoInvestimento={tipo} />\r\n        </div>\r\n\r\n        <AdsenseParagrafo1 />\r\n        <br />\r\n        <div className={styles.divInfo}>\r\n          <H2 texto={'RENTABILIDADE MS A MS'} />\r\n          <RetornoDetalhado\r\n            data={data}\r\n            tipoInvestimento={tipo}\r\n            funcionalidade={'comparador'}\r\n          />\r\n        </div>\r\n\r\n        <AdsenseParagrafo3 />\r\n        <br />\r\n\r\n        <div className={styles.divInfo}>\r\n          <H2 texto={'EVOLUO DA RENTABILIDADE'} />\r\n          <RetornoEvolucao data={data} tipoInvestimento={tipo} />\r\n        </div>\r\n\r\n        {tipo == 'acoes' ? (\r\n          <div className={styles.divInfo}>\r\n            <H2 texto={'INDICADORES FUNDAMENTALISTAS'} />\r\n            <Fundamentus2\r\n              tickers={acoesComparacao.replaceAll('|', '-').slice(0, -1)}\r\n            />\r\n\r\n            <AdsenseParagrafo5 />\r\n          </div>\r\n        ) : null}\r\n      </>\r\n    );\r\n  else return null;\r\n};\r\n\r\nexport default ComparadorResultados;\r\n","C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Components\\Comparador\\ComparadorItems.js",["1056"],"import React from 'react';\r\nimport styles from '../../Pages/Comparador/Comparador.module.css';\r\n\r\nconst ComparadorItems = (props) => {\r\n  const [mostraItems, setMostraItems] = React.useState(false);\r\n  React.useEffect(() => {\r\n    if (props.listaProdutos.split('|').length > 0) {\r\n      setMostraItems(true);\r\n    } else {\r\n      setMostraItems(false);\r\n    }\r\n  }, []);\r\n\r\n  function getLista() {\r\n    return (\r\n      <div className={styles.containerProdutos}>\r\n        {props.listaProdutos.split('|').map(\r\n          (item) =>\r\n            item.length > 2 && (\r\n              <div className={styles.itemProdutos}>\r\n                <p>{item}</p>\r\n              </div>\r\n            ),\r\n        )}\r\n      </div>\r\n    );\r\n  }\r\n\r\n  return <div>{mostraItems && getLista()}</div>;\r\n};\r\n\r\nexport default ComparadorItems;\r\n","C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Pages\\Comparador\\ComparadorFundos.js",[],"C:\\Users\\dougl\\OneDrive\\Documentos\\projetos\\aws\\reactjs\\dozeroapp\\src\\Pages\\Melhores\\FundosImobiliarios.js",["1057","1058","1059","1060"],"import React from 'react';\r\nimport Head from '../../Components/Tags/Head';\r\nimport H2 from '../../Components/Tags/H2';\r\nimport Link from '../../Components/Tags/Link';\r\nimport Paragrafo from '../../Components/Tags/Paragrafo';\r\nimport { Container } from 'react-bootstrap';\r\nimport Loading from '../../Components/Helper/Loading';\r\nimport styles from './Melhores.module.css';\r\nimport styleLista from '../../Components/Tags/Lista.module.css';\r\nimport stylePost from '../../Components/Tags/Post.module.css';\r\nimport {\r\n  AWS_GET_FII_ACOES,\r\n  AWS_GET_INVESTIMENTOS,\r\n} from '../../Components/Integracoes/Api';\r\nimport useFetch from '../../Hooks/useFetch';\r\n\r\nimport AcoesResultadosFinal from '../../Components/Bolsa/Acoes/AcoesResultadosFinal';\r\nimport RatingStars from '../../Components/Rating/RatingStars';\r\nimport BreadcumbH1 from '../../Components/Tags/BreadcumbH1';\r\nimport ItemDuvida from '../../Components/Tags/ItemDuvida';\r\nimport isMobile from '../../Components/Helper/Mobile';\r\nimport Categorias from '../../Components/Categorias/Categorias';\r\nimport AdsenseParagrafo1 from '../../Components/Adsense/AdsenseParagrafo1';\r\nimport AdsenseParagrafo3 from '../../Components/Adsense/AdsenseParagrafo3';\r\nimport AdsenseParagrafo5 from '../../Components/Adsense/AdsenseParagrafo5';\r\n\r\nconst FundosImobiliarios = () => {\r\n  const mobile = isMobile();\r\n  const { data, loading, error, request, setData } = useFetch();\r\n  const dataSetores = [\r\n    'HIBRIDO',\r\n    'HOSPITAL',\r\n    'HOTEL',\r\n    'LAJES CORPORATIVAS',\r\n    'LOGISTICA',\r\n    'OUTROS',\r\n    'RESIDENCIAL',\r\n    'SHOPPINGS',\r\n    'TITULOS E VAL. MOB.',\r\n  ];\r\n\r\n  const dataOrdenacao = [\r\n    'maior_preco',\r\n    'maior_dividendo',\r\n    'maior_retorno_mes',\r\n    'maior_retorno_ano',\r\n    'maior_retorno_12mes',\r\n    'menor_preco',\r\n    'menor_dividendo',\r\n    'menor_retorno_mes',\r\n    'menor_retorno_ano',\r\n    'menor_retorno_12mes',\r\n  ];\r\n  const [sortType, setSortType] = React.useState(null);\r\n  const [ordenacao, setOrdenacao] = React.useState('Selecione');\r\n  const [setores, setSetores] = React.useState(['Selecione']);\r\n  const [dados, setDados] = React.useState(null);\r\n  const [filtro, setFiltro] = React.useState(false);\r\n  // const [filtroUtilizado, setFiltroUtilizado] = React.useState(false);\r\n\r\n  React.useEffect(() => {\r\n    if (data) {\r\n      setDados(data);\r\n      const sortArray = (type) => {\r\n        setOrdenacao(type);\r\n\r\n        const types = {\r\n          maior_preco: 'preco_atual',\r\n          maior_dividendo: 'dividendo',\r\n          maior_retorno_mes: 'retorno_mes',\r\n          maior_retorno_ano: 'retorno_ano',\r\n          maior_retorno_12mes: 'retorno_12mes',\r\n          menor_preco: 'preco_atual',\r\n          menor_dividendo: 'dividendo',\r\n          menor_retorno_mes: 'retorno_mes',\r\n          menor_retorno_ano: 'retorno_ano',\r\n          menor_retorno_12mes: 'retorno_12mes',\r\n        };\r\n\r\n        const sortProperty = types[type];\r\n\r\n        let sorted;\r\n        if (dados) {\r\n          sorted = [...dados];\r\n          if (type.includes('maior')) {\r\n            sorted = [...dados].sort(\r\n              (a, b) => b[sortProperty] - a[sortProperty],\r\n            );\r\n          } else {\r\n            sorted = [...dados].sort(\r\n              (a, b) => a[sortProperty] - b[sortProperty],\r\n            );\r\n          }\r\n        } else {\r\n          sorted = [...data];\r\n\r\n          if (type.includes('maior')) {\r\n            sorted = [...data].sort(\r\n              (a, b) => b[sortProperty] - a[sortProperty],\r\n            );\r\n          } else {\r\n            sorted = [...data].sort(\r\n              (a, b) => a[sortProperty] - b[sortProperty],\r\n            );\r\n          }\r\n        }\r\n\r\n        setDados(sorted);\r\n      };\r\n      sortArray(sortType);\r\n    } else {\r\n      window.scroll(0, 0);\r\n      var d = new Date();\r\n      var hora = d.getHours();\r\n      var url = '';\r\n\r\n      // console.log(hora);\r\n      if (hora === 18 || hora === 19 || hora === 20) {\r\n        const { url, options } = AWS_GET_FII_ACOES('fii');\r\n        request(url, options);\r\n      } else {\r\n        const { url, options } = AWS_GET_INVESTIMENTOS('FII', 'TODOS');\r\n        request(url, options);\r\n      }\r\n    }\r\n  }, [request, sortType]);\r\n\r\n  function getResultados() {\r\n    if (dados) {\r\n      return <AcoesResultadosFinal data={dados} isAcao={false} />;\r\n    } else {\r\n      return <AcoesResultadosFinal data={data} isAcao={false} />;\r\n    }\r\n  }\r\n\r\n  function handleSubmit(event) {\r\n    event.preventDefault();\r\n    const btnFiltros = document.getElementById('btn-filtros');\r\n    if (btnFiltros.innerText === 'FILTROS +') {\r\n      setFiltro(true);\r\n      btnFiltros.innerText = 'FILTROS -';\r\n    } else {\r\n      setFiltro(false);\r\n      btnFiltros.innerText = 'FILTROS +';\r\n    }\r\n  }\r\n\r\n  function handleCheckedSetor(setor) {\r\n    return setores.includes(setor);\r\n  }\r\n\r\n  function hanfleClickPesquisar(event) {\r\n    event.preventDefault();\r\n    // setFiltroUtilizado(true);\r\n\r\n    setOrdenacao('Selecione');\r\n\r\n    let dados3 = data;\r\n\r\n    if (setores.length > 1) {\r\n      dados3 = dados3.filter((registro) =>\r\n        setores.some((r) => registro.setor.split('|').includes(r)),\r\n      );\r\n    }\r\n\r\n    console.log(setores.length);\r\n    if (setores.length === 1) {\r\n      // setFiltroUtilizado(false);\r\n      setDados(data);\r\n    } else {\r\n      setDados(dados3);\r\n    }\r\n    setFiltro(false);\r\n  }\r\n\r\n  function handleChange({ target }) {\r\n    if (target.id.includes('chk_setor')) {\r\n      if (target.checked) {\r\n        setSetores([...setores, target.value]);\r\n      } else {\r\n        setSetores(setores.filter((setor) => setor !== target.value));\r\n      }\r\n    }\r\n  }\r\n\r\n  function retrotnaFiltros() {\r\n    return (\r\n      <div className={styles.containerFiltros}>\r\n        <div className={styles.itemFiltros}>\r\n          <form onSubmit={handleSubmit}>\r\n            <button id=\"btn-filtros\" className={styles.button}>\r\n              FILTROS +\r\n            </button>\r\n\r\n            {filtro && (\r\n              <div className={styles.form}>\r\n                <label className={styles.labelForm}>Setores</label>\r\n                <div className={styles.containerForm}>\r\n                  {dataSetores.map((setor) => (\r\n                    <div className={styles.itemForm}>\r\n                      <label key={setor}>\r\n                        <input\r\n                          id={'chk_setor_' + setor}\r\n                          type=\"checkbox\"\r\n                          value={setor}\r\n                          checked={handleCheckedSetor(setor)}\r\n                          onChange={handleChange}\r\n                        />\r\n                        <span className={styles.spanCheckList}>{setor}</span>\r\n                      </label>\r\n                    </div>\r\n                  ))}\r\n                </div>\r\n\r\n                <div className={styles.divAplicarFIltro}>\r\n                  <button\r\n                    id=\"btn-filtros2\"\r\n                    className={styles.button}\r\n                    onClick={hanfleClickPesquisar}\r\n                  >\r\n                    APLICAR FILTROS\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            )}\r\n          </form>\r\n        </div>\r\n        <div className={styles.itemFiltros}>{retornaListaOrdenacao()}</div>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  function retornaListaOrdenacao() {\r\n    return (\r\n      <select\r\n        className={styles.dropdownList}\r\n        id=\"seletorOrdenacao\"\r\n        onChange={(e) => setSortType(e.target.value)}\r\n        value={ordenacao}\r\n      >\r\n        <option className={s