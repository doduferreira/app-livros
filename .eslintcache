);\r\n  const [valorTotalPago, setValorTotalPago] = React.useState(null);\r\n  const [valorTipo, setValorTipo] = React.useState(null);\r\n  const [contador, setContador] = React.useState(1);\r\n  const [taxaMedia, setTaxaMedia] = React.useState(6.6);\r\n  const [erro, seterro] = React.useState(false);\r\n  const formatter = new Intl.NumberFormat('pt-BR', {\r\n    style: 'currency',\r\n    currency: 'BRL',\r\n    minimumFractionDigits: 2,\r\n  });\r\n\r\n  React.useEffect(() => {\r\n    if (params.valor > 0) {\r\n      setInputParcelas(12);\r\n      setInputValorEmprestimo(params.valor);\r\n      setInputTaxa(taxaMedia);\r\n      calculaJurosEmprestimo(params.valor, 12, taxaMedia, tipoTaxa);\r\n    }\r\n  }, []);\r\n\r\n  function getTitle() {\r\n    let titulo = '';\r\n\r\n    if (params.valor > 0) {\r\n      titulo = 'Simulador de Empréstimo de ' + params.valor + ' Reais';\r\n    } else {\r\n      titulo = 'Simulador de Empréstimo';\r\n    }\r\n\r\n    return titulo;\r\n  }\r\n\r\n  function getMetaDescription() {\r\n    let meta = '';\r\n\r\n    if (params.valor > 0) {\r\n      meta =\r\n        'Simule um Empréstimo de ' +\r\n        params.valor +\r\n        ' Reais. Confira o valor das parcelas, quantas parcelas irá pagar e qual o valor total gasto no pagamento no empréstimo';\r\n    } else {\r\n      meta =\r\n        'Simulador de Empréstimo. Confira o valor das parcelas, quantas parcelas irá pagar e qual o valor total gasto no pagamento no empréstimo.';\r\n    }\r\n\r\n    return meta;\r\n  }\r\n\r\n  function calculaJurosEmprestimo(valorEmprestimo, parcelas, taxa, tipo) {\r\n    if (taxa) {\r\n      taxa = parseFloat(taxa.toString().replace(',', '.'));\r\n    }\r\n\r\n    if (valorEmprestimo) {\r\n      valorEmprestimo = parseFloat(\r\n        valorEmprestimo.toString().replace(',', '.'),\r\n      );\r\n    }\r\n\r\n    if (valorEmprestimo > 0 && parcelas > 0 && taxa > 0) {\r\n      seterro(false);\r\n      setContador(contador + 1);\r\n      setValorTipo(t